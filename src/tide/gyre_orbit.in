&constants
/

&model
	model_type = 'EVOL'
	file = 'spb.mesa'
	file_format = 'MESA'
/

&tide
	q = 0.5
	e = 0.3
	omega_static = 0.
	k_max = 5
	l_max = 4
/

&osc
	nonadiabatic = .TRUE.
/

&num
	diff_scheme = 'COLLOC_GL2'
/

&grid
	n_inner = 5
	alpha_osc = 10
	alpha_exp = 2
/

&scan
        freq_min = 0.38
        freq_max = 0.44
        freq_min_units = 'NONE'
        freq_max_units = 'NONE'
        n_freq = 100
/
