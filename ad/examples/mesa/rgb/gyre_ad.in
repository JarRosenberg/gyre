&coeffs
	coeffs_type = 'EVOL'
	file = 'dupret_model_B.mesa'
	file_format = 'MESA'
/

&osc
	l = 1
/

&num
	ivp_solver_type = 'MAGNUS_GL2'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 38,
	freq_max = 42,
	freq_units = 'UHZ'
	n_freq = 200,
/

&shoot_grid
	op_type = 'CREATE_CLONE'
/

&shoot_grid
	op_type = 'RESAMP_DISPERSION'
	alpha_osc = 5
	alpha_exp = 1
/

&shoot_grid
	op_type = 'RESAMP_CENTER'
	alpha_osc = 5
	alpha_exp = 1
	n = 5
/

&recon_grid
	op_type = 'CREATE_CLONE'
/

&output
/
