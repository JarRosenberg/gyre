&constants
/

&model
	model_type = 'EVOL'
	file = 'bcep.mesa'
	file_format = 'MESA'
/

&osc
	l = 0
/

&osc
	l = 1
/

&osc
	l = 2
/

&osc
	l = 3
/

&num
	ivp_solver_type = 'MAGNUS_GL2'
	n_iter_max = 250
/

&scan
	grid_type = 'LINEAR'
	freq_min = 1
	freq_max = 20.0
	n_freq = 200
/

&shoot_grid
/

&recon_grid
	op_type = 'CREATE_MIDPOINT'
/

&output
	summary_file = 'gyre_nad.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,omega_im'
/
