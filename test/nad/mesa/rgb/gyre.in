&constants
/

&model
	model_type = 'EVOL'
	file = 'rgb.mesa'
	file_format = 'MESA'
/

&mode
	l = 0
/

&mode
	l = 1
/

&mode
	l = 2
/

&mode
	l = 3
/

&osc
	nonadiabatic = .TRUE.
/

&num
	diff_scheme = 'COLLOC_GL4'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 39,
	freq_max = 39.5
	freq_units = 'UHZ'
	n_freq = 200,
/

&grid
	base_type = 'MODEL'
	n_center = 5
	alpha_osc = 10
	alpha_exp = 2
/

&ad_output
/

&nad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,omega_int'
/
