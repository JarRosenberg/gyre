&constants
/

&model
	model_type = 'EVOL'
	file = 'rgb.mesa'
	file_format = 'MESA'
/

&mode
	l = 0
/

&mode
	l = 1
/

&mode
	l = 2
/

&osc
	nonadiabatic = .TRUE.
/

&rot
/

&num
	diff_scheme = 'COLLOC_GL4'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 41
	freq_max = 42
	freq_min_units = 'UHZ'
	freq_max_units = 'UHZ'
	n_freq = 50
/

&grid
	w_osc = 10.
	w_exp = 2.
	w_ctr = 10.
/

&ad_output
	summary_file = 'summary.h5'
	summary_item_list = 'l,n_pg,n_p,n_g,omega'
/

&nad_output
/
