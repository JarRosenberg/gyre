&constants
/

&model
	model_type = 'EVOL'
	file = 'dcep.mesa'
	file_format = 'MESA'
/

&mode
	l = 0
/

&osc
	inner_bound = 'ZERO'
	nonadiabatic = .TRUE.
/

&num
	diff_scheme = 'MAGNUS_GL2'
/

&scan
	grid_type = 'LINEAR'
	freq_min = 1.0
	freq_max = 10.0
	n_freq = 50
/

&grid
	alpha_osc = 10
	alpha_exp = 2
	x_i = 0.0015
/

&ad_output
/

&nad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega'
/
