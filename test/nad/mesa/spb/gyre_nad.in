&constants
/

&model
	model_type = 'EVOL'
	file = 'spb.mesa'
	file_format = 'MESA'
/

&mode
	l = 0
/

&mode
	l = 1
/

&mode
	l = 2
	X_n_pg_min = -50
/

&mode
	l = 3
	X_n_pg_min = -50
/

&osc
/

&num
	ivp_solver_type = 'MAGNUS_GL2'
	n_iter_max = 100
/

&scan
	grid_type = 'INVERSE'
	freq_min = 0.15
	freq_max = 1
	n_freq = 200
/

&scan
	grid_type = 'LINEAR'
	freq_min = 1
	freq_max = 10
	n_freq = 100
/

&shoot_grid
/

&recon_grid
	op_type = 'CREATE_MIDPOINT'
/

&output
	summary_file = 'gyre_nad.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,omega_im'
/
