&constants
	G_GRAVITY = 6.67259d-8
/

&model
	model_type = 'EVOL'
	file = 'evolved'
	file_format = 'WDEC'
/

&mode
	l = 1
/

&mode
	l = 2
/

&osc
/

&rot
/

&num
	diff_scheme = 'COLLOC_GL4'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 0.00067
	freq_max = 0.01
	freq_min_units = 'HZ'
	freq_max_units = 'HZ'
	n_freq = 100
/

&grid
	w_osc = 10
	w_exp = 2
	w_ctr = 10
/

&ad_output
	summary_file = 'summary.h5'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,freq'
	freq_units = 'HZ'
/

&nad_output
/
