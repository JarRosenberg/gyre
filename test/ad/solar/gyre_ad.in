&constants
/

&model
	model_type = 'EVOL'
	file = 'fgong.l5bi.d.15c'
	file_format = 'FGONG'
/

&mode
	l = 1000
/

&osc
	outer_bound_type = 'JCD'
/

&num
	ivp_solver = 'MAGNUS_GL6'
/

&scan
	grid_type = 'LINEAR'
	freq_min = 3000
	freq_max = 5000
	freq_units = 'UHZ'
	n_freq = 10
/

&shoot_grid
	op_type = 'CREATE_CLONE'
/

&shoot_grid
	op_type = 'RESAMP_DISPERSION'
	alpha_osc = 5.
	alpha_exp = 1.
/

&recon_grid
/

&output
	summary_file = 'gyre_ad.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,freq'
	freq_units = 'UHZ'
/
