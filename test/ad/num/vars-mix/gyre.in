&constants
/

&model
	model_type = 'HOM'
/

&mode
	l = 0
	tag = 'l=0'
/

&mode
	l = 0
	tag = 'l=0r'
/

&mode
	l = 1
	tag = 'l=1'
/

&mode
	l = 2
	tag = 'l=2'
/

&mode
	l = 3
	tag = 'l=3'
/

&osc
	variables_set = 'MIX'
	reduce_order = .FALSE. 
/

&osc
	variables_set = 'MIX'
	reduce_order = .TRUE.
	tag_list = 'l=0r'
/

&num
	diff_scheme = 'COLLOC_GL6'
/

&scan
        grid_type = 'LINEAR'
        freq_min = 0.9
        freq_max = 18.7
        n_freq = 30
        tag_list = 'l=0r,l=0'
/

&scan
        grid_type = 'LINEAR'
        freq_min = 2.1
        freq_max = 19.5
        n_freq = 30
        tag_list = 'l=1'
/

&scan
        grid_type = 'LINEAR'
        freq_min = 2.8
        freq_max = 20.4
        n_freq = 30
        tag_list = 'l=2'
/

&scan
        grid_type = 'LINEAR'
        freq_min = 3.4
        freq_max = 21.2
        n_freq = 30
        tag_list = 'l=3'
/

&grid
	base_type = 'GEOM'
	s_base = 1000.
	n_base = 1000
/

&output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,E_norm'
/
