&constants
/

&model
	model_type = 'HOM'
/

&mode
	l = 1
/

&osc
	cowling_approx = .TRUE.
/

&num
	ivp_solver = 'COLLOC_GL6'
/

&scan
	grid_type = 'LINEAR'
	freq_min = 0.1
	freq_max = 21.0
	n_freq = 100
/

&shoot_grid
	op_type = 'CREATE_GEOM'
	s = 1000.
	n = 1000
/

&recon_grid
/

&output
	summary_file = 'gyre_ad.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega'
/
