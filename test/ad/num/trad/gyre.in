&constants
/

&model
	model_type = 'HOM'
	uniform_rot = .TRUE.
	Omega_rot = 1.0
/

&mode
	l = 1
	m = -1
/

&mode
	l = 1
	m = 0
/

&mode
	l = 1
	m = 1
/

&osc
	rotation_method = 'TRAD'
/

&num
	diff_scheme = 'COLLOC_GL6'
/

&scan
	grid_type = 'LINEAR'
	freq_min = 0.8
	freq_max = 11.0
	freq_frame = 'COROT_O'
	grid_frame = 'COROT_O'
	n_freq = 20
/

&grid
	base_type = 'GEOM'
	s_base = 1000.
	n_base = 1000
/

&output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,m,n_pg,n_p,n_g,omega'
/
