&constants
/

&model
	model_type = 'POLY'
        file = 'poly.h5'
/

&mode
	l = 1
/

&osc
/

&num
	diff_scheme = 'COLLOC_GL6'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 0.02
	freq_max = 1.0
	n_freq = 100
	freq_units = 'GRAVITY_DELTA'
/

&grid
/

&ad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,m,n_pg,n_p,n_g,omega'
/

&nad_output
/
